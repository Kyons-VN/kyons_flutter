  "version": "0.2.0",
  "configurations": [
    {
      "name": "Mobile dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart"
    },
    {
      "name": "Web dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev_web.dart",
      "args": ["--web-port", "12345"]
    },
    {
      "name": "Web stg",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_stg_web.dart",
      "args": ["--web-port", "12345"]
    },
    {
      "name": "main.dart",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "kyons_flutter (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
